import java.util.*;
class Main {  
  public static void main(String args[]) { 
    List<Double> list=new ArrayList<Double>();
    list.add(1.5);  
    list.add(2.9);  
    list.add(3.7);  
    list.add(3.1);
    list.add(5.10);
    
    int i = 0;
    while(i<list.size()){
    System.out.println(list.get(i));
     i++;
    }
      }
 
  } 
